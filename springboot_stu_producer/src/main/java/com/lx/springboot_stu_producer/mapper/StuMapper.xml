<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lx.springboot_stu_producer.mapper.StuMapper">

	<select id="queryStus" resultType="student">
		select * from stus
	</select>

	<delete id="delStu" parameterType="int">
          delete from stus where id =#{id}
	</delete>

</mapper>